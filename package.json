{
  "name": "dr-alex-website",
  "version": "1.0.0",
  "description": "Dr. Alex Kisitu website with Claude AI chat integration",
  "main": "server/claude-service.js",
  "type": "module",
  "scripts": {
    "start": "npm run kill-ports && concurrently --kill-others \"npm run backend\" \"npm run frontend\"",
    "backend": "node server/claude-service.js",
    "frontend": "http-server -p 5000 -c-1",
    "dev": "npm run kill-ports && concurrently --kill-others \"nodemon server/claude-service.js\" \"http-server -p 5000 -c-1\"",
    "kill-ports": "npx kill-port 3000 5000",
    "test": "jest",
    "test:server": "node server/test-server.js",
    "test:chat": "http-server -p 5500 -c-1 -o /chat-test.html"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.4",
    "http-server": "^14.1.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "kill-port": "^2.0.1",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
